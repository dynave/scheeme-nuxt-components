{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recess-order"
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "no-descending-specificity": null,
    "function-name-case": [ "lower", {
      "ignoreFunctions": ["/findColorInvert/", "/^get.*$/"]
    }],
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extend",
      ],
    }]
  }
}
